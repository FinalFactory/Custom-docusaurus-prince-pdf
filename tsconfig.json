{
    "compilerOptions": {
      "target": "ES6",
      "module": "es2020",
      "moduleResolution": "node",
      "outDir": "./dist",
      "strict": false,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"]
  }